name: nyc_airbnb
conda_env: conda.yml

entry_points:
  main:
    parameters:
      steps:
        description: Comma-separated list of steps to execute (useful for debugging)
        type: str
        default: all
      etl.sample:
        description: Sample file name
        type: str
        default: sample1.csv

    command: >
      python src/main.py hydra.run.dir=. hydra.output_subdir=null etl.sample=${etl.sample} steps=${steps}
